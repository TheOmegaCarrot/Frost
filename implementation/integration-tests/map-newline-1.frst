def arrs = [
    [
        '1', '2', '3'
    ],
    [
        '4', '5', '6'
    ],
    [
        '7', '8', '9'
    ],
]

def add_arr = fn arr -> {
    def nums = map arr with to_int
    reduce nums with plus
}

def arr_sums = map arrs with add_arr

def expect = [6, 15, 24]

print(pretty(arr_sums))
print('==')
print(pretty(expect))

assert(deep_equal(arr_sums, expect))
